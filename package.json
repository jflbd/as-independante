    "start:prod": "vite preview",
    "lint": "eslint .",
    "check": "npm run lint && tsc --noEmit",
    "convert-images": "node scripts/convert-to-webp.js",
    "optimize-images": "npm run convert-images && node scripts/optimize-images.js",
    "generate-favicons": "node scripts/generate-favicons.js",
